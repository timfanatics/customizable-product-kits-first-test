        <t t-name="bom_to_sale_order.ExpandListRenderer.RecordRow" t-inherit="web.ListRenderer.RecordRow" t-inherit-mode="primary" owl="1">
            <!--<xpath expr="//tr[hasclass('o_data_row')]" position="attributes">
                <attribute name="class" position="add" separator=" ">o_section_expand</attribute>
            </xpath>-->


            <xpath expr="//tr//td[hasclass('o_list_record_remove')]" position="after">
                <td t-if="record.data.display_type=='line_section'" class="o_section_expand text-center"
                    t-on-click.stop="(ev) => this.onClickExpandCollapse(ev,record)"
                    tabindex="-1"
                >
                    <button class="fa fa-chevron-up"
                        id="expand_collapse_section"
                        name="expand"
                        aria-label="Expand/Collapse Section"
                        tabindex="-1"
                    />
                </td>
             </xpath>
        </t>

